{
    "name": "maveio/statamic-mave",
    "type": "statamic-addon",
    "description": "A Statamic addon to use Mave videos in your Statamic site.",
    "license": "MIT",
    "require": {
        "statamic/cms": "^4.0",
        "firebase/php-jwt": "^6.9"
    },
    "require-dev": {
        "php": "^8.1",
        "laravel/framework": "^10.8",
        "statamic/cms": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "StatamicMave\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic mave.io",
            "description": "A Statamic addon to use Mave videos in your Statamic site."
        },
        "laravel": {
            "providers": [
                "StatamicMave\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
